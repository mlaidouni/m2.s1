#  les ressources de type asws:Roi 

PREFIX asws: <http://www.asws.com/rois#>
PREFIX rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT ?nom ?nomepoux ?dreine ?depoux
WHERE {
  ?roi a asws:Reine ;
	   asws:nom ?nom ;
	   asws:datenaiss ?dreine ;
	   asws:epoux [asws:nom ?nomepoux ;
	   				asws:datenaiss ?depoux];
		FILTER(year(?dreine) = year(?depoux)) .
}